<% if current_user %>
  <div id='button'>
    <%= button_to 'Log Out', root_path, method: :delete%>
  </div>
<% else %>
  <div id='button'>
    <%= button_to 'Create New User', register_path, method: :get %>
  </div>
<br>
<br>
  <div id='button'>
    <%= button_to 'Login', login_path, method: :get %>
  </div>
<% end %>

<br>

<section id='existing_users'>
  <h2>Existing Users</h2>
  <% @users.each do |user| %>
    <div id='user-<%= user.id %>'>
      <p>
        <%= "#{user.email}" %>
      </p>
    </div>
  <% end %>
</section>
